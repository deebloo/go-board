{
  "name": "go-board",
  "version": "2.0.4",
  "private": false,
  "description": "Go board",
  "type": "module",
  "exports": {
    "./*": {
      "import": "./target/build/*"
    }
  },
  "files": [
    "target/build"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deebloo/go-board.git"
  },
  "keywords": [],
  "author": "deebloo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deebloo/go-board/issues"
  },
  "scripts": {
    "start": "vite",
    "build": "run-s build:*",
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:vite": "vite build",
    "build-lib": "tsc -p tsconfig.build.json",
    "test": "run-s test:*",
    "test:build": "tsc -p tsconfig.spec.json",
    "test:wtr": "wtr \"target/spec/**/**.spec.js\" --node-resolve"
  },
  "dependencies": {
    "@joist/di": "2.0.1",
    "@joist/observable": "2.0.1",
    "@joist/query": "2.0.1",
    "@joist/styled": "2.0.1"
  },
  "devDependencies": {
    "@open-wc/testing": "^3.1.2",
    "@web/test-runner": "^0.13.27",
    "firebase-tools": "^11.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rollup-plugin-minify-html-literals": "^1.2.6",
    "typescript": "^4.9.5",
    "vite": "^2.9.1",
    "wireit": "^0.9.5"
  }
}
