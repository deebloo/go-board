{
  "name": "go-board",
  "version": "2.1.3",
  "private": false,
  "description": "Go board",
  "type": "module",
  "exports": {
    "./*": {
      "import": "./target/*"
    }
  },
  "files": [
    "target"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deebloo/go-board.git"
  },
  "keywords": [],
  "author": "deebloo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deebloo/go-board/issues"
  },
  "scripts": {
    "start": "vite",
    "build": "wireit",
    "build:tsc": "wireit",
    "build:vite": "vite build",
    "build:rollup": "wireit",
    "test": "wireit"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "build:tsc",
        "build:rollup",
        "build:vite"
      ]
    },
    "build:tsc": {
      "command": "tsc --build --pretty",
      "clean": "if-file-deleted",
      "files": [
        "src/**",
        "tsconfig.json"
      ],
      "output": [
        "target/**",
        "tsconfig.tsbuildinfo"
      ]
    },
    "build:rollup": {
      "command": "rollup -c rollup.config.js",
      "dependencies": [
        "build:tsc"
      ]
    },
    "test": {
      "command": "wtr \"target/**/**.spec.js\" --node-resolve --puppeteer",
      "dependencies": [
        "build:tsc"
      ]
    }
  },
  "devDependencies": {
    "@open-wc/testing": "^3.1.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@web/test-runner": "^0.15.1",
    "@web/test-runner-puppeteer": "^0.11.0",
    "firebase-tools": "^11.7.0",
    "minify-html-literals": "^1.3.5",
    "prettier": "^2.8.4",
    "rollup": "^3.20.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "wireit": "^0.9.5"
  }
}
